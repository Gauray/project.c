#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int i,n,a[10000];
    cin>>n;
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for( i=n-1; i>=0; i--){
      printf("%d ",a[i]);
      }
      
       
    return 0;
}
